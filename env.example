# MLOps Project Environment Variables
# Copia este archivo a .env y modifica los valores según necesites

# Airflow Configuration
AIRFLOW_UI_PORT=8080
MLFLOW_UI_PORT=5001
POSTGRES_PORT=5432

# Database Configuration
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_DB=airflow

# Airflow Admin User
AIRFLOW_ADMIN_USERNAME=admin
AIRFLOW_ADMIN_PASSWORD=admin
AIRFLOW_ADMIN_EMAIL=admin@example.com

# MLflow Configuration
MLFLOW_BACKEND_STORE_URI=file:/mlruns
MLFLOW_DEFAULT_ARTIFACT_ROOT=/mlruns

# Model Configuration
MODEL_NAME=RandomForestRegressor
EXPERIMENT_NAME=mlops-experiment

# Security (genera una nueva clave para producción)
AIRFLOW_FERNET_KEY=your-fernet-key-here
